<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>

    <PREFIX>
        <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
        <LABEL>owl</LABEL>
    </PREFIX>
     <PREFIX>
        <NAMESPACE>http://www.tomtom.com/ontologies/traces#</NAMESPACE>
        <LABEL>tomtom</LABEL>
    </PREFIX>
    <SOURCE>
        <ID>tomtomsourcedata1</ID>
        <ENDPOINT>source.nt</ENDPOINT>
        <VAR>?x</VAR>
        <PAGESIZE>2000</PAGESIZE>
        <RESTRICTION>?x a tomtom:Trace</RESTRICTION>
     <PROPERTY>tomtom:hasPoint</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:hasTimestamp RENAME timeStamp</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:label RENAME label</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:lat RENAME lat</PROPERTY>
     <PROPERTY>tomtom:hasPoint/tomtom:long RENAME long</PROPERTY>
     <PROPERTY>tomtom:hasPoint/tomtom:hasSpeed/tomtom:velocityValue RENAME velocityValue</PROPERTY>
     <TYPE>NT</TYPE> 
    </SOURCE>
    <TARGET>
        <ID>tomtomtargetdata1</ID>
        <ENDPOINT>target.nt</ENDPOINT>
        <VAR>?y</VAR>
        <PAGESIZE>2000</PAGESIZE>
        <RESTRICTION>?y a tomtom:Trace</RESTRICTION>
     <PROPERTY>tomtom:hasPoint</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:hasTimestamp RENAME timeStamp</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:label RENAME label</PROPERTY> 
     <PROPERTY>tomtom:hasPoint/tomtom:lat RENAME lat</PROPERTY>
     <PROPERTY>tomtom:hasPoint/tomtom:long RENAME long</PROPERTY>
     <PROPERTY>tomtom:hasPoint/tomtom:hasSpeed/tomtom:velocityValue RENAME velocityValue</PROPERTY>
     <TYPE>NT</TYPE> 
    </TARGET>
	<METRIC>AND(AND(AND(AND(cosine(x.label, y.label)|1.0,trigram(x.timeStamp, y.timeStamp)|1.0),top_equals(x.lat,y.lat)|1.0),top_equals(x.long,y.long)|1.0),euclidean(x.velocityValue, y.velocityValue)|1.0)</METRIC>
    <ACCEPTANCE>
        <THRESHOLD>1.0</THRESHOLD>
        <FILE>absolute_mapping_value.nt</FILE>
        <RELATION>owl:sameAs</RELATION>
    </ACCEPTANCE>
    <REVIEW>
        <THRESHOLD>0.5</THRESHOLD>
        <FILE>absolute_mapping_almost_value.nt</FILE>
        <RELATION>owl:sameAs</RELATION>
    </REVIEW>

    <EXECUTION>
        <REWRITER>default</REWRITER>
        <PLANNER>default</PLANNER>
        <ENGINE>default</ENGINE>
    </EXECUTION>

    <OUTPUT>TAB</OUTPUT>
</LIMES>
